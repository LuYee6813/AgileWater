{
    "include": [
        "src/**/*.d.ts",
        "src/**/*.js",
        "src/**/*.ts",
        "src/**/*.svelte"
    ],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "$lib": [
                "src/lib"
            ],
            "$lib/*": [
                "src/lib/*"
            ]
        }
    }
}